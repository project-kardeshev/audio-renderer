{
    "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
    "mainEntryPointFilePath": "<projectFolder>/dist/typings-temp/index.d.ts",
    "bundledPackages": [],
    "compiler": {

    },
    "apiReport": {
        "enabled": false
    },
    "docModel": {
        "enabled": false
    },
    "dtsRollup": {
        "enabled": true,
        "untrimmedFilePath": "<projectFolder>/dist/react-audio-wave.d.ts"
    },
    "tsdocMetadata": {
        "enabled": false
    },
    "messages": {
        "compilerMessageReporting": {
            "default": {
                "logLevel": "warning"
            }
        },
        "extractorMessageReporting": {
            "default": {
                "logLevel": "warning"
            },
            "ae-missing-release-tag": {
                "logLevel": "none"
            }
        },
        "tsdocMessageReporting": {
            "default": {
                "logLevel": "warning"
            }
        }
    }
}
